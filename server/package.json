{
  "name": "brev.ly-server",
  "version": "1.0.0",
  "description": "API para encurtador de URLs - Brev.ly",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch --env-file .env src/server.ts",
    "build": "tsup src --out-dir dist",
    "start": "node dist/server.js",
    "db:migrate": "drizzle-kit push:pg",
    "db:generate": "drizzle-kit generate:pg",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "url-shortener",
    "fastify",
    "typescript"
  ],
  "author": "Jo√£o Carvalho",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.8",
    "drizzle-kit": "^0.20.14",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.515.0",
    "@fastify/cors": "^9.0.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "drizzle-orm": "^0.29.4",
    "fastify": "^4.26.0",
    "postgres": "^3.4.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  }
}
